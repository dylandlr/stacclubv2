      "[data-popper-placement^='bottom'] > [data-popper-arrow]": "-top-2",
      "[data-popper-placement^='left'] > [data-popper-arrow]": "-right-2",
      "[data-popper-placement^='right'] > [data-popper-arrow]": "-left-2",
